<!-- admin page js-->
<script src="/js/toastr.min.js"></script>
<script>

    <% if (messages.info ) {%>
        <% messages.info.forEach(function (info) {%>
            toastr.info('<%= info.msg || info.toString() %>');
        <%})%>
    <%}%>
    <% if (messages.errors ) {%>
        <% messages.errors.forEach(function (error) {%>
            toastr.error('<%= error.msg || error.toString()%>');
        <%})%>
    <%}%>
    <% if (messages.success) {%>
        toastr.success('<%= messages.success%>');
    <%}%>

</script>